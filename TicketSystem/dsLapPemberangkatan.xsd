                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT     ADM_KONSUMEN.NAMA, TRX_PENJUALAN.QTY_PENUMPANG, TRX_PENJUALAN.ONGKOS, TRX_PENJUALAN.JUMLAH, REF_TIPE_KENDARAAN.KAS, 
                      TRX_PENDAPATAN_DATANG.PENDAPATAN, REF_TIPE_KENDARAAN.SETOR_WAJIB, ADM_JADWAL.TGL_BERANGKAT, TRX_PENJUALAN.JUMLAH -(TRX_PENJUALAN.QTY_PENUMPANG * ADM_KOMISI.KOMISI) AS KOMISI, 
                      REF_INDEX_GAJI.INDEX_GAJI
FROM         REF_TIPE_KENDARAAN INNER JOIN
                      REF_KENDARAAN ON REF_TIPE_KENDARAAN.ID_TIPE_KENDARAAN = REF_KENDARAAN.ID_TIPE_KENDARAAN INNER JOIN
                      ADM_JADWAL ON REF_KENDARAAN.ID_KENDARAAN = ADM_JADWAL.ID_KENDARAAN INNER JOIN
                      TRX_PENJUALAN INNER JOIN
                      ADM_KONSUMEN ON TRX_PENJUALAN.ID_KONSUMEN = ADM_KONSUMEN.ID_KONSUMEN ON 
                      ADM_JADWAL.ID_JADWAL = TRX_PENJUALAN.ID_JADWAL INNER JOIN
                      REF_SUPIR ON ADM_JADWAL.ID_SUPIR = REF_SUPIR.ID_SUPIR INNER JOIN
                      ADM_KOMISI ON TRX_PENJUALAN.ID_KOMISI = ADM_KOMISI.ID_KOMISI INNER JOIN
                      REF_INDEX_GAJI ON ADM_JADWAL.ID_INDEX_GAJI = REF_INDEX_GAJI.ID_INDEX_GAJI LEFT OUTER JOIN
                      TRX_PENDAPATAN_DATANG ON ADM_JADWAL.ID_JADWAL = TRX_PENDAPATAN_DATANG.ID_JADWAL
WHERE     (ADM_JADWAL.ID_JADWAL = @Id)</CommandText>

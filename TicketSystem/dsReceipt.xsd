                             (SELECT        CONVERT(NVARCHAR(10), NO_KURSI) + ', ' AS 'data()'
                               FROM            TRX_PENJUALAN_DETAIL
                               WHERE        (ID_PENJUALAN = TRX_PENJUALAN.ID_PENJUALAN) FOR XML PATH('')) AS NO_KURSI

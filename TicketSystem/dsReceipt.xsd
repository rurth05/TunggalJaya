                    <CommandText>SELECT        TRX_PENJUALAN.TGL_PENJUALAN, ADM_KONSUMEN.NAMA, TRX_PENJUALAN.QTY_PENUMPANG, TRX_PENJUALAN.ONGKOS, TRX_PENJUALAN.JUMLAH, 
